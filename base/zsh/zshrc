# Customize to your needs...
if [[ -z $TMUX  ]]; then
  export PATH=$PATH:$HOME/bin
  export PATH=$PATH:$HOME/node_modules/.bin
  export PATH=$PATH:/usr/bin:/bin:/usr/sbin:/sbin:/usr/local/bin:/opt/local/bin:/opt/local/sbin:/usr/local/sbin:/usr/games
  export PATH=$PATH:$HOME/.local/bin
fi

for zsh_source in ~/.config/zsh/config.d/*.zsh; do
  source $zsh_source
done

cd_to_most_recently_opened_directory
